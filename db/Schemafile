create_table "shops", force: :cascade do |t|
  t.integer  "shop_id"
  t.string   "name"
  t.string   "address"
  t.datetime "created_at"
  t.datetime "updated_at"
end

create_table "images", force: :cascade do |t|
  t.integer  "shop_id"
  t.string   "file"
  t.datetime "created_at"
  t.datetime "updated_at"
end
